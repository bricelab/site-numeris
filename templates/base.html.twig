<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	{% block metadescription %}{% endblock %}
	<!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

	<title>NUMERIS SARL</title>
	<!-- Favicons -->
	<link href="{{ absolute_url(asset("theme/img/N.png")) }}" rel="icon">
	<link href="{{ absolute_url(asset("theme/img/N02.png")) }}" rel="apple-touch-icon">

	<!-- Google font -->
	<link href="https://fonts.googleapis.com/css?family=Montserrat:400,700%7CVarela+Round" rel="stylesheet">
    <!--link rel="stylesheet" href="https://bootswatch.com/3/flatly/bootstrap.min.css"-->

	<!-- Bootstrap -->
	<link type="text/css" rel="stylesheet" href="{{ absolute_url(asset("theme/css/bootstrap.min.css")) }}" />

	<!-- Owl Carousel -->
	<link type="text/css" rel="stylesheet" href="{{ absolute_url(asset("theme/css/owl.carousel.css")) }}" />
	<link type="text/css" rel="stylesheet" href="{{ absolute_url(asset("theme/css/owl.theme.default.css")) }}" />

	<!-- Magnific Popup -->
	<link type="text/css" rel="stylesheet" href="{{ absolute_url(asset("theme/css/magnific-popup.css")) }}" />

	<!-- Font Awesome Icon -->
	<link rel="stylesheet" href="{{ absolute_url(asset("theme/css/font-awesome.min.css")) }}">

	<!-- Custom stlylesheet -->
	<link type="text/css" rel="stylesheet" href="{{ absolute_url(asset("theme/css/style.css")) }}" />

	<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
