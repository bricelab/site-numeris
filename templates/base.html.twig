					<!-- footer follow -->
					<ul class="footer-follow">
						<li><a href="https://facebook.com/numerisbenin"><i class="fa fa-facebook"></i></a></li>
						<li><a href="https://twitter.com/numerisbenin"><i class="fa fa-twitter"></i></a></li>
						<!--<li><a href="#"><i class="fa fa-google-plus"></i></a></li>
						<li><a href="#"><i class="fa fa-instagram"></i></a></li>
						<li><a href="#"><i class="fa fa-linkedin"></i></a></li>
						<li><a href="#"><i class="fa fa-youtube"></i></a></li>-->
					</ul>
					<!-- /footer follow -->

					<!-- footer copyright -->
					<div class="footer-copyright">
						<p>COPYRIGHT © 2019. TOUS DROITS RÉSERVÉS. RÉALISÉ PAR <a href="http://www.numeris.bj" target="_blank">NUMERIS</a></p>
					</div>
					<!-- /footer copyright -->

				</div>

			</div>
			<!-- /Row -->

		</div>
		<!-- /Container -->

	</footer>
	<!-- /Footer -->

	<!-- Back to top -->
	<div id="back-to-top"></div>
	<!-- /Back to top -->

	<!-- Preloader -->
	<div id="preloader">
		<div class="preloader">
			<span></span>
			<span></span>
			<span></span>
			<span></span>
		</div>
	</div>
	<!-- /Preloader -->

	<!-- jQuery Plugins -->
	<script type="text/javascript" src="{{ absolute_url(asset("theme/js/jquery.min.js")) }}"></script>
	<script type="text/javascript" src="{{ absolute_url(asset("theme/js/bootstrap.min.js")) }}"></script>
	<script type="text/javascript" src="{{ absolute_url(asset("theme/js/owl.carousel.min.js")) }}"></script>
	<script type="text/javascript" src="{{ absolute_url(asset("theme/js/jquery.magnific-popup.js")) }}"></script>
	<script type="text/javascript" src="{{ absolute_url(asset("theme/js/main.js")) }}"></script>

